{
  "name": "beer-recipe-adapter",
  "version": "0.0.1",
  "description": "Adapt any beer recipe to your needs",
  "main": "index.js",
  "scripts": {
    "browserify": "./node_modules/browserify/bin/cmd.js -t reactify -t babelify index.js -o bundle.js",
    "watchify": "./node_modules/watchify/bin/cmd.js -t reactify -b babelify index.js -o bundle.js",
    "lint": "./node_modules/eslint/bin/eslint.js .",
    "test": "jest --noHighlight"
  },
  "jest": {
    "globals": {
      "__DEV__": true
    },
    "scriptPreprocessor": "./node_modules/babel-jest",
    "testDirectoryName": "test",
    "unmockedModulePathPatterns": ["./node_modules/"]
  },
  "author": "Sean Gillespie",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sgillespie/beer-recipe-adapter.git"
  },
  "keywords": [
    "homebrew",
    "recipe"
  ],
  "bugs": {
    "url": "https://github.com/sgillespie/beer-recipe-adapter/issues"
  },
  "homepage": "https://github.com/sgillespie/beer-recipe-adapter#readme",
  "dependencies": {
    "babelify": "^6.3.0",
    "eslint": "^1.5.1",
    "jquery": "^2.1.4",
    "lodash": "^3.10.1",
    "react": "^0.13.3",
    "react-bootstrap": "^0.25.2",
    "react-redux": "^2.1.2",
    "reactify": "^1.1.1",
    "redux": "^3.0.0",
    "redux-thunk": "^1.0.0",
    "watchify": "^3.4.0"
  },
  "devDependencies": {
    "babel-eslint": "^4.1.3",
    "babel-jest": "^5.3.0",
    "eslint": "^1.5.1",
    "eslint-config-airbnb": "^0.1.0",
    "eslint-plugin-react": "^3.5.1",
    "jest-cli": "^0.5.8",
    "reactify": "^1.1.1"
  }
}
