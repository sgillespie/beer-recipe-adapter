{
  "extends": "airbnb",
  "globals": {
    "describe": true,
    "it": true,
    "beforeEach": true
  },
  "rules": {
    "space-before-function-paren": [2, "always"],
    "func-names": 0,
    "one-var": 0,
    "indent": [2, 2, {
      "SwitchCase": 1,
      "VariableDeclarator": {
        "var": 2,
        "let": 2,
        "const": 3,
      }
    }],
    "no-unused-expressions": 0,
    "no-use-before-define": [2, "nofunc"],
    "id-length": 0
  }
}
